# Local configuration for RLM + LiteLLM runner
# Copy to .env and fill in values.

# LiteLLM proxy base URL (e.g., your gateway)
LITELLM_API_BASE=https://asgard.bhishmaraj.org

# LiteLLM API key (DO NOT COMMIT REAL KEYS)
LITELLM_API_KEY=

# Default model route for your proxy
LITELLM_MODEL=gemini/gemini-2.5-flash-lite

# Some upstream imports look for this; keep a harmless placeholder
OPENAI_API_KEY=placeholder

